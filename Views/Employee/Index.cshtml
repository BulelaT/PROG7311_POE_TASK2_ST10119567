@model PROG7311_POE_TASK2_ST10119567.Models.FarmerModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/EmployeeLayout.cshtml";
}

<h2 style="font-family: Papyrus;">Current List of Farmers</h2>
<link href="~/Content/TableFormat.css" rel="stylesheet">
<table class="CustomiseTable">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Username</th>
            <th>Email</th>
            <th>Start Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.farmerList)
        {
        <tr>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.Username</td>
            <td>@item.Email</td>
            <td>@item.StartDate</td>
        </tr>
        }
    </tbody>

</table>


